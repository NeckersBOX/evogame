{
  "env": {
    "browser": true,
    "node": true
  },
  "parser": "babel-eslint",
  "rules": {
    "quotes": [1, "single"],
    "strict": [2, "never"],
    "react/jsx-uses-react": 2,
    "react/jsx-uses-vars": 2,
    "no-unused-vars": [
      1,
      { "varsIgnorePattern": "h" }
    ],
    "no-undef": 2
  },
  "plugins": [
    "react"
  ]
}
